{
  "name": "codegen-evaluator",
  "version": "1.0.0",
  "description": "Code generation snippet evaluator",
  "type": "module",
  "scripts": {
    "eval": "tsx src/evaluate.ts",
    "test": "jest",
    "build": "tsc",
    "seed": "tsx scripts/seed_fake_candidates.js"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "isolated-vm": "^4.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/tasks/**/*.spec.js"],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}